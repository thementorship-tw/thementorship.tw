{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/strict-type-checked",
    "plugin:@typescript-eslint/stylistic-type-checked",
    "next/core-web-vitals",
    "next/typescript",
    "prettier",
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "projectService": true,
  },
  "rules": {
    "require-await": "off",
    "@typescript-eslint/require-await": "off",
  },
}
